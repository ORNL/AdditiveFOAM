FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {
        box (-1 -1 -4e-5) (1 1 1);

        fieldValues
        (
            volScalarFieldValue alpha.powder 1
        );
    }
);


