FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     additiveFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.004;

deltaT          1e-07;

writeControl    adjustableRunTime;

writeInterval   0.001;

purgeWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.5;

maxDi           1;

maxAlphaCo      1;

functions
{
    solidificationData
    {
        libs ("libadditiveFoamFunctionObjects.so");
        
        type solidificationData;
                
        box         (-1 -1 -1) (1 1 1);
        isoValue    1620;
    }
/*
    ExaCA
    {
        libs ("libadditiveFoamFunctionObjects.so");
        
        type ExaCA;
                
        box         (0 -0.0001 -0.0002) (0.002 0.0001 0);
        dx          2.5e-6;
        isoValue    1620;
    }
*/
}

