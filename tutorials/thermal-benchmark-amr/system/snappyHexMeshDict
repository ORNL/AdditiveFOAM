FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          snappyHexMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#includeEtc     "caseDicts/mesh/generation/snappyHexMeshDict.cfg"

castellatedMesh true;

snap            false;

addLayers       false;

geometry
{
    part
    {
        type            triSurfaceMesh;
        file            "part.stl";
    }
}

castellatedMeshControls
{
    features        ( { file "part.eMesh" ; level 0 ; } );
    refinementSurfaces
    {
        part
        {
            level           ( 0 0 );
            patchInfo
            {
                type            wall;
                inGroups        ( wall );
            }
        }
    }
    refinementRegions
    {
        part
        {
            mode            inside;
            levels          ( ( 0 0 ) );
        }
    }
    nCellsBetweenLevels 3;
    locationInMesh  ( 0.0725097 0.0935744 0.00865557 );
}

snapControls
{
}

addLayersControls
{
    layers
    {
    }
    relativeSizes   true;
    expansionRatio  1.2;
    finalLayerThickness 0.5;
    minThickness    0.001;
}

mergeTolerance  1e-15;


