FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

includedAngle   180;

subsetFeatures
{
    nonManifoldEdges yes;
    openEdges       yes;
}

trimFeatures
{
    minElem         0;
    minLen          0;
}

writeObj        yes;

surfaces        ( "part.stl" );


